<Window x:Class="TheHastyFinn.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheHastyFinn"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Populate FScore" HorizontalAlignment="Left" Margin="10,288,0,0" VerticalAlignment="Top" Width="100" Click="button_Click" RenderTransformOrigin="0.493,0.611"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="273" VerticalAlignment="Top" Width="100" Margin="10,10,0,0" SelectionChanged="listBox_SelectionChanged"/>
        <Button x:Name="button1" Content="Refresh" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="115,288,0,0" Click="button1_Click"/>

    </Grid>
</Window>
